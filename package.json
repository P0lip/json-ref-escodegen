{
  "name": "json-ref-escodegen",
  "version": "0.0.0",
  "description": "Performant, lean and bare-simple JSON Schema $ref resolver",
  "keywords": [
    "json",
    "json-pointer",
    "json-$ref",
    "jsonschema",
    "json-schema",
    "json-resolver",
    "json-ref-resolver",
    "$ref",
    "openapi",
    "swagger",
    "dereference",
    "resolve"
  ],
  "homepage": "https://github.com/P0lip/json-ref-escodegen",
  "bugs": {
    "url": "https://github.com/P0lip/json-ref-escodegen/issues"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist/index.cjs",
    "src/**"
  ],
  "typings": "./src/index.d.ts",
  "main": "./dist/index.cjs",
  "exports": {
    "import": "./src/index.mjs",
    "require": "./dist/index.cjs"
  },
  "license": "Apache-2.0",
  "author": "P0lip",
  "repository": {
    "type": "git",
    "url": "https://github.com/P0lip/json-ref-escodegen"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "lint": "eslint --cache --cache-location .cache/ --ext .mjs src/",
    "test": "NODE_ENV=test c8 mocha --experimental-modules --config .mocharc ./**/__tests__/**/*.test.mjs",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "c8": "^7.2.0",
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "mocha": "^8.0.1",
    "mocha-chai-snapshot": "^1.0.0",
    "mocha-each": "^2.0.1",
    "prettier": "^2.0.5",
    "rollup": "^2.20.0",
    "sinon": "^9.0.2"
  },
  "dependencies": {
    "@sindresorhus/fnv1a": "^2.0.1",
    "astring": "^1.4.3"
  }
}
