{
  "name": "json-ref-escodegen",
  "version": "0.0.0",
  "description": "Performant, lean and bare-simple JSON Schema $ref resolver",
  "keywords": [
    "json",
    "json-pointer",
    "json-$ref",
    "jsonschema",
    "json-schema",
    "json-resolver",
    "json-ref-resolver",
    "$ref",
    "openapi",
    "swagger",
    "dereference",
    "resolve"
  ],
  "homepage": "https://github.com/P0lip/json-ref-escodegen",
  "bugs": {
    "url": "https://github.com/P0lip/json-ref-escodegen/issues"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist/index.cjs",
    "src/**"
  ],
  "typings": "./src/index.d.ts",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./src/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./runtime/": {
      "import": "./src/runtime/",
      "require": "./dist/runtime/"
    }
  },
  "license": "Apache-2.0",
  "author": "P0lip",
  "repository": {
    "type": "git",
    "url": "https://github.com/P0lip/json-ref-escodegen"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "lint": "eslint --cache --cache-location .cache/ --ext .mjs src/",
    "pretest": "npm run build",
    "test": "NODE_ENV=test c8 mocha --experimental-modules --config .mocharc ./**/__tests__/**/*.test.{cjs,mjs}",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "c8": "^7.2.0",
    "chai": "^4.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "mocha": "^8.0.1",
    "mocha-chai-snapshot": "^1.0.0",
    "mocha-each": "^2.0.1",
    "prettier": "^2.0.5",
    "rollup": "^2.33.1",
    "sinon": "^9.0.2"
  },
  "dependencies": {
    "@sindresorhus/fnv1a": "^2.0.1",
    "astring": "^1.4.3"
  }
}
